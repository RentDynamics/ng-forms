{
  "name": "@rd/core",
  "version": "0.0.0-development",
  "main": "bundles/core.umd.js",
  "repository": {
    "url": "https://github.com/RentDynamics/ng-core"
  },
  "typings": "public_api.d.ts",
  "private": false,
  "author": {
    "name": "rd"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.9.4",
    "npm": ">=5.6.0"
  },
  "scripts": {
    "build": "npx ng-packagr -p ng-package-ci.json",
    "commit": "git-cz",
    "test": "printf '\n \\e[0;32m ----- Max, your push worked! ----- \\e[0m\n\n'",
    "prepush": "npm test",
    "semantic-release": "cd ./dist && semantic-release"
  },
  "readme": "README.md",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "moment": "^2.22.2",
    "jssha": "^2.3.1",
    "@types/jssha": "^0.0.29"
  }
}
